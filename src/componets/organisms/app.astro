---

import Intro from "../molecules/intro.astro";
import Social from "../molecules/socials.astro";
import Presence from "../molecules/presence.astro";
import Icon from "../atoms/icon.astro";
import About from "../molecules/about.astro";

---

<section>
        <div class="flex flex-row justify-between items-start mt-24 mb-24 mx-6 sm:mx-[10px] md:mx-[25px] xl:mx-[35px] 2xl:mx-[500px]">
                <section>
                        <Intro></Intro>
                        <Social></Social>
                </section>
                
                <Icon  lanyardProfilePicture="https://media.tenor.com/iUHKMSbav30AAAAC/aes-pfp.gif"> </Icon>
        </div>

        <div class="flex flex-row justify-between mx-6 sm:mx-[10px] md:mx-[20px] xl:mx-[35px] 2xl:mx-[500px]">
                <Presence></Presence>
                <About></About>
        </div>
</section>

